HOST=0.0.0.0
PORT=8080
DATA_DIR=/data
DB_PATH=/data/licenses.db
DEFAULT_PRODUCT_ID=default-plugin

# BuiltByBit external license key webhook secret
BBB_SECRET=change_this_bbb_secret

# Secret used by Coliseum plugin in X-Coliseum-Secret header
VALIDATE_SECRET=change_this_validate_secret

# Secret for admin API endpoints (/admin/*)
ADMIN_SECRET=change_this_admin_secret

# Password for web console (/console)
PANEL_PASSWORD=change_this_console_password

# Keep false in production
ALLOW_ISSUE_WITHOUT_SECRET=false

# JSON map from BuiltByBit resource id to your plugin product key
# Example: {"123456":"coliseum","987654":"survivalcore"}
BBB_RESOURCE_PRODUCT_MAP={}
